{
    "name": "spie/google-service-provider",
    "description": "Google service provider for Laravel.",
    "keywords": ["laravel", "google", "services"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">7.1.3",
        "google/apiclient": "^2.2",
        "illuminate/support": "^5.6",
        "illuminate/container": "^5.6"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "SPie\\GoogleServiceProvider\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "SPie\\GoogleServiceProvider\\Providers\\LaravelGoogleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
